<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white">



    <ImageView
        android:id="@+id/home_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/home_4_svgrepo_com"
        android:layout_marginTop="@dimen/dimen_10dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/register_button"
        android:layout_marginEnd="@dimen/dimen_50dp"/>

    <TextView
        android:id="@+id/home_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/home_button"
        app:layout_constraintEnd_toStartOf="@id/register_text"
        android:text="Home"
        android:layout_marginTop="@dimen/dimen_20dp"
        android:layout_marginEnd="@dimen/dimen_50dp"
        android:textSize="@dimen/dimen_25dp"
        android:textColor="@color/black"/>


    <ImageView
        android:id="@+id/register_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/billing_button"
        android:layout_marginEnd="@dimen/dimen_80dp"
        android:layout_marginTop="@dimen/dimen_10dp"
        android:src="@drawable/add_square_svgrepo_com"
        app:layout_constraintTop_toTopOf="parent" />




    <TextView
        android:id="@+id/register_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/register_button"
        app:layout_constraintEnd_toStartOf="@+id/billing_text"
        android:layout_marginEnd="@dimen/dimen_70dp"
        android:text="Registration"
        android:textSize="@dimen/dimen_25dp"
        android:textColor="@color/black"/>


    <ImageView
        android:id="@+id/billing_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/lab_button"
        android:layout_marginEnd="@dimen/dimen_100dp"
        android:layout_marginTop="@dimen/dimen_10dp"
        android:src="@drawable/billing"
        app:layout_constraintTop_toTopOf="parent" />




    <TextView
        android:id="@+id/billing_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/billing_button"
        app:layout_constraintEnd_toStartOf="@+id/lab_text"
        android:layout_marginEnd="@dimen/dimen_80dp"
        android:layout_marginTop="@dimen/dimen_50dp"
        android:text="Billing"
        android:textSize="@dimen/dimen_25dp"
        android:textColor="@color/black"/>


    <ImageView
        android:id="@+id/lab_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/more_button"
        android:layout_marginEnd="@dimen/dimen_80dp"
        android:layout_marginTop="@dimen/dimen_15dp"
        android:src="@drawable/lab"
        app:layout_constraintTop_toTopOf="parent" />




    <TextView
        android:id="@+id/lab_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/lab_button"
        app:layout_constraintEnd_toStartOf="@+id/more_text"
        android:layout_marginTop="@dimen/dimen_50dp"
      android:layout_marginEnd="@dimen/dimen_80dp"
        android:text="Lab"
        android:textSize="@dimen/dimen_25dp"
        android:textColor="@color/black"/>


    <ImageView
        android:id="@+id/more_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="@dimen/dimen_80dp"
        android:layout_marginTop="@dimen/dimen_10dp"
        android:src="@drawable/more"
        app:layout_constraintTop_toTopOf="parent" />




    <TextView
        android:id="@+id/more_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/more_button"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="@dimen/dimen_80dp"
        android:layout_marginTop="@dimen/dimen_50dp"
        android:text="More"
        android:textSize="@dimen/dimen_25dp"
        android:textColor="@color/black"/>






</androidx.constraintlayout.widget.ConstraintLayout>